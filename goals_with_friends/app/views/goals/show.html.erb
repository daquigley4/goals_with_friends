<div class="page-header">
  <%= link_to goals_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_goal_path(@goal), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show goal</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @goal.title %></dd>

  <dt>Due date:</dt>
  <dd><%= @goal.due_date %></dd>

  <dt>Completed:</dt>
  <dd><%= @goal.completed %></dd>

</dl>
